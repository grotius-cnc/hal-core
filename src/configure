##############################################################################
# Subsection 3.5 - check for GTK                                             #
# FIXME: allow it to be enabled or disabled command line                     #
##############################################################################

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for glib" >&5
$as_echo_n "checking for glib... " >&6; }
if pkg-config glib-2.0 >/dev/null 2>&1; then
  GLIB_VER=`pkg-config glib-2.0 --modversion`
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes - $GLIB_VER" >&5
$as_echo "yes - $GLIB_VER" >&6; }
  GLIB_CFLAGS=`pkg-config glib-2.0 --cflags`
  GLIB_LIBS=`pkg-config glib-2.0 --libs`
else
  as_fn_error $? "no -- required until somebody makes glib optional" "$LINENO" 5
fi


# Check whether --enable-gtk was given.
if test "${enable_gtk+set}" = set; then :
  enableval=$enable_gtk;
	case "$enableval" in
	Y*|y*)
	    BUILD_GTK=yes ;;
	*)
	    BUILD_GTK=no ;;
	esac

else
  BUILD_GTK=yes
fi


if test "$BUILD_GTK" = "yes"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for GTK 2.4.0 or above" >&5
$as_echo_n "checking for GTK 2.4.0 or above... " >&6; }
  if pkg-config gtk+-2.0 --atleast-version 2.4.0 >/dev/null 2>&1; then
    GTK_VER=`pkg-config gtk+-2.0 --modversion`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes - $GTK_VER" >&5
$as_echo "yes - $GTK_VER" >&6; }
    GTK_CFLAGS=`pkg-config gtk+-2.0 --cflags`
    GTK_LIBS=`pkg-config gtk+-2.0 --libs`
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking for libgnomeprintui-2.2" >&5
$as_echo_n "checking for libgnomeprintui-2.2... " >&6; }
    if pkg-config --exists libgnomeprintui-2.2; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
      GTK_CFLAGS="$GTK_CFLAGS `pkg-config --cflags libgnomeprintui-2.2`"
      GTK_LIBS="$GTK_LIBS `pkg-config --libs libgnomeprintui-2.2`"
      HAVE_GNOMEPRINT=yes
    else
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: no -- printing from classicladder will not be possible" >&5
$as_echo "no -- printing from classicladder will not be possible" >&6; }
      HAVE_GNOMEPRINT=
    fi
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    as_fn_error $? "GTK2 missing.  Install it or specify --disable-gtk to skip the parts of LinuxCNC that depend on GTK" "$LINENO" 5
  fi
fi








